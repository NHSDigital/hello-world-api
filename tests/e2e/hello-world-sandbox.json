	"item": [
		{
			"name": "Hello World - Open access",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "99e7adf5-5ad6-4220-bf83-0a901e29ba9d",
						"exec": [
							"pm.environment.set(\"expected_greeting\", \"Hello World!\");"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "e5c85463-44ec-4937-9598-a32c502c270f",
						"exec": [
							"pm.test(\"Returns status code 200 OK\", () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Returns correct mock response\", () => {",
							"    const responseJson = pm.response.json()",
							"    const expectedGreeting = pm.environment.get(\"expected_greeting\")",
							"    pm.expect(responseJson.message).to.eql(expectedGreeting)",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/hello/world",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"hello",
						"world"
					]
				}
			},
			"response": []
		},
		{
			"name": "Hello World - _ping response with deployed version",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "60d752ea-398c-40bd-82c8-1061edc960bb",
						"exec": [
							"pm.test(\"Status code\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var versionRegex = /v[0-9]+\\.[0-9]+\\.[0-9]+-\\w+/;",
							"pm.test(\"Body has version\", function () {",
							"    pm.expect(versionRegex.test(pm.response.text())).to.be.true;",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
